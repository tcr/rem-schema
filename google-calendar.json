{
  "2": {
    "name": "Google Calendar",
    "docs": "http://code.google.com/apis/calendar/data/2.0/developers_guide_protocol.html",

    "base": "https://www.google.com/calendar/feeds",
    "params": {
      "v": "2",
      "alt": "jsonc"
    },

    "formats": {
      "xml": {},
      "json": {"params": {"alt": "jsonc"}}
    },

    "auth": {
      "type": "oauth",
      "version": "1.0A",
      "requestEndpoint": "https://www.google.com/accounts/OAuthGetRequestToken",
      "accessEndpoint": "https://www.google.com/accounts/OAuthGetAccessToken",
      "authorizeEndpoint": "https://www.google.com/accounts/OAuthAuthorizeToken",
      "params": {
        "scope": ["http://www.google.com/calendar/feeds"]
      },
      "scopeSeparator": " ",
      "validate": "/default/owncalendars/full",
      "oob": true
    }
  }
}